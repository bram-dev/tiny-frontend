{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run generate:css -- --minify",
    "generate:css": "tailwindcss -o ./dist/styles/tailwind.css",
    "preview": "vite preview",
    "deploy": "npm run build && node deploy/deploy.js",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@cazoo/eslint/react"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "devDependencies": {
    "@cazoo/eslint-plugin-eslint": "^1.0.2",
    "@tiny-frontend/deploy-cloudflare": "^0.0.3",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.13",
    "dotenv": "^14.2.0",
    "eslint": "^7.32.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  }
}
